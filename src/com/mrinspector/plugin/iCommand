    commandhandler35();
    commandhandler36();
    commandhandler37();
    commandhandler38();
    commandhandler39();
    commandhandler40();
    commandhandler41();
    getLogger().log(Level.INFO, "Beta Version!");
    getServer().getPluginManager().registerEvents(this, this);
  }

  public void commandhandler()
  {
    setExecutor("heal", new Heal());
  }

